{
  "_format_version": "3.0",
  "_transform": true,
  "services": [    
	{
      "host": "google.com",
      "name": "nsi-service",
      "port": 80,
      "protocol": "http",
      "retries": 0,
      "read_timeout": 3600000,
      "routes": [
        {
          "name": "catch-all-route",
          "expression": "http.path ^= \"/\"",
          "strip_path": false,
		  "priority": 0
        },
		{
          "name": "nsi-exceptions-not-supported-by-new-service-route",		 
          "expression": "http.path ^= \"/rest/data\" && (http.queries.dimensionAtObservation != \"AllDimensions\" || http.queries.asOf != \"\" || http.queries.includeHistory != \"\" || http.queries.updateAfter != \"\" || (http.queries.format != \"jsondata\" && http.queries.format != \"csv\" && http.queries.format != \"csvfile\" && http.queries.format != \"csvfilewithlabels\"))",
          "strip_path": false,
		  "priority": 100
        }
      ]
    },
	{
      "host": "httpbin.konghq.com",
      "name": "data-service",
      "port": 80,
      "protocol": "http",
      "path": "/anything",
      "retries": 0,
      "read_timeout": 3600000,
      "routes": [
        {
          "name": "ds-data-route",
          "expression": "http.path ^= \"/rest/data\" && (http.headers.accept ~ \"application/vnd\\\\.sdmx\\\\.data\\\\+(json|csv)\" || http.queries.format == \"jsondata\" || http.queries.format == \"csv\" || http.queries.format == \"csvfile\" || http.queries.format == \"csvfilewithlabels\")",
          "strip_path": false,
		  "priority": 90
        },
		{
          "name": "ds-availableconstraint-route",
          "expression": "http.path ^= \"/rest/availableconstraint\" && http.headers.accept ~ \"structure\\\\+json\"",
          "strip_path": false,
		  "priority": 90
        }
      ]
    }
  ]
}